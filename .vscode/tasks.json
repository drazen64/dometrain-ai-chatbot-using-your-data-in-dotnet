{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build 2-semantic-search",
      "type": "shell",
      "command": "dotnet",
      "args": [
        "build",
        "${workspaceFolder}/2-semantic-search/2-semantic-search.csproj"
      ],
      "problemMatcher": [
        "$msCompile"
      ],
      "group": "build"
    },
    {
      "label": "run 2-semantic-search",
      "type": "shell",
      "command": "dotnet",
      "args": [
        "run",
        "--project",
        "${workspaceFolder}/2-semantic-search/2-semantic-search.csproj"
      ],
      "problemMatcher": [
        "$msCompile"
      ],
      "group": "test"
    }
  ]
}
